{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "securityhub:*",
        "config:*",
        "cloudtrail:*",
        "guardduty:*",
        "kms:DescribeKey",
        "kms:GetKeyRotationStatus",
        "kms:ListAliases",
        "kms:Encrypt",
        "kms:Decrypt",
        "s3:*",
        "ec2:*",
        "iam:Get*",
        "iam:List*",
        "logs:*",
        "cloudwatch:*",
        "organizations:Describe*",
        "organizations:List*",
        "lambda:*"
      ],
      "Resource": "*"
    }
  ]
}
